{% extends 'base.html.twig' %}

{% block title %}
Search
{% endblock %}

{% block content %}

<h2>Search Results</h2>

<table border="1">
{% for result in results %}
<tr>
    <td valign="top"><a href="{{ result.url }}">{{ result.name }}</a></td>
    <td>{{ result.description|raw }}</td>
</tr>
{% endfor %}
</table>

{% endblock %}