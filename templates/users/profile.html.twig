{% extends 'base.html.twig' %}

{% block title %}
User {{ user.username }}
{% endblock %}

{% block content %}
<h1>{{ user.username }}</h1>
<p><b>Role:</b> {{ user.role }}</p>
<p><b>Created at:</b> {{ user.createdAt|date('M j, Y H:i:s') }}</p>
<p><b>Updated at:</b> {{ user.updatedAt|date('M j, Y H:i:s') }}</p>
{% endblock %}
